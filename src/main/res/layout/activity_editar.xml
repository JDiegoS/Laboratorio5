<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Editar">

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="15"

        android:layout_centerInParent="true"
        android:hint="Nombre"
        android:id="@+id/nombre1"
        />

    <EditText
        android:id="@+id/Telefono1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="15"
        android:hint="Telefono"
        android:layout_below="@+id/nombre1"
        android:layout_centerHorizontal="true"

        />
    <EditText
        android:id="@+id/correo1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="15"
        android:hint="Correo Electronico"
        android:layout_below="@+id/Telefono1"
        android:layout_centerHorizontal="true"

        />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/correo1"
        android:layout_centerHorizontal="true"
        android:text="Crear"
        android:id="@+id/crear1"
        />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/crear1"
        android:layout_centerHorizontal="true"
        android:text="Regresar"
        android:id="@+id/regresar1"
        />
    <ImageView
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_centerHorizontal="true"
        android:layout_above="@+id/nombre1"
        android:src="@drawable/ic_action_name"
        />

</RelativeLayout>